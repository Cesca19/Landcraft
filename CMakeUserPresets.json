{
    "version": 4,
    "vendor": {
        "conan": {}
    },
    "include": [
        "build/generators/CMakePresets.json"
    ],
    "configurePresets": [
        {
            "name": "default",
            "description": "Default configuration",
            "cacheVariables": {
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
            },
            "binaryDir": "${sourceDir}/build"
        },
        {
            "name": "compile-commands",
            "description": "Configuration with compile commands",
            "inherits": "default",
            "cacheVariables": {
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
            }
        }
    ]
}